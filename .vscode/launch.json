{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "gdb",
      "request": "launch",
      "name": "Flash and Debug Jlink",
      "target": "./build/TLS3001_SPI.elf",
      "cwd": "${workspaceFolder}",
      "gdbpath": "xtensa-esp32-elf-gdb",
      "autorun": [
        "target remote :3333",
        "mon reset halt",
        "mon program_esp32 /home/mikbeng/esp/esp32_TLS3001_spi/build/TLS3001_SPI.bin 0x10000 verify",
        "mon reset halt",
        "flushregs",
        "thb app_main",
        "c"
      ]
      //"preLaunchTask": "copy-binary-towin"
    }
  ]
}